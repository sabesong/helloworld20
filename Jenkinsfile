pipeline { 
    agent any
    stages {
      stage ('build'){
        steps {
          echo "build step"
            sleep 4
        }
      
    }
     stages {
      stage ('test'){
        steps {
          echo "test step"
            sleep 5
   }
          
}
    stages {
      stage ('deploy'){
        steps {
          echo "deploy war file"
            sleep 10
            
        }
    }
 }       
